bind C-Tab focus
bind C-q only
bind C-1 exec $browser
bind C-2 exec emacs

exec setxkbmap -option keypad:pointerkeys
exec xkbset ma 60 10 10 5 2

exec xcompmgr &
exec $HOME/.fehbg
exec rpws init 4 -k
exec wmane LG3D
